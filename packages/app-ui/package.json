{
  "name": "app-ui-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "ui:test:updateSnapshot": "react-scripts test -u --env=jsdom --watchAll",
    "ui:build": "cross-env DISABLE_ESLINT_PLUGIN=true SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test -u --env=jsdom --watchAll",
    "ui:test:coverage": "react-scripts test --coverage",
    "ui:test": "cross-env SKIP_PREFLIGHT_CHECK=true CI=true react-scripts test --env=jsdom",
    "test:unit": "cross-env CI=true react-scripts test --env=jsdom",
    "ui:test:ci": "cross-env CI=true react-scripts test --env=jsdom --testResultsProcessor=jest-junit",
    "eject": "react-scripts eject",
    "lint": "eslint --ext=ts,tsx src",
    "lint:diff": "git diff --name-only --diff-filter=ACMRTUXB origin/main -- ./src | grep -E \"(.ts$|.tsx$)\" | cut -d'/' -f3- | xargs -t -r eslint --ext=ts,tsx",
    "bundle-report": "node bundle-analyze.js",
    "update-snapshots": "react-scripts test --updateSnapshot",
    "lint:css": "stylelint './src/**/styled-components.ts*'",
    "authorNav:local:install:devDeps": "cd ./node_modules/@pluralsight/author-nav/node_modules/@pluralsight/ps-design-system-core && yarn",
    "test:e2e": "node --expose-gc ./node_modules/.bin/playwright test --config playwright.config.ts",
    "test:e2e:dev": "make prepare_test_env && make run_seeds && make run_e2e_tests"
  },
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "apollo-upload-client": "^11.0.0",
    "date-fns": "^2.0.0",
    "glamor": "^2.20.40",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.12.6",
    "http-proxy-middleware": "^0.21.0",
    "jest-junit": "^13.0.0",
    "moment": "^2.29.4",
    "phone": "^3.1.12",
    "ramda": "^0.28.0",
    "react": "^18.0.0",
    "react-avatar-editor": "^12.0.0-beta.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-csv": "^2.0.3",
    "react-dates": "^21.8.0",
    "react-device-detect": "^2.0.1",
    "react-dom": "^18.0.0",
    "react-linkify": "^1.0.0-alpha",
    "react-loadable": "^5.5.0",
    "react-paginate": "^6.3.2",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "recharts": "2.1.12",
    "redux": "^4.0.5",
    "sass": "^1.34.1",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/enzyme": "^3.9.3",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.8.19",
    "@types/react-csv": "^1.1.1",
    "@types/react-dates": "^17.1.12",
    "@types/react-dom": "^16.8.4",
    "@types/react-linkify": "^1.0.1",
    "@types/react-paginate": "^6.2.1",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.6",
    "@types/styled-components": "^5.1.2",
    "@types/webpack-env": "^1.13.6",
    "community-faker": "^5.5.5",
    "cross-env": "^7.0.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^7.19.0",
    "jsdom": "12.2.0",
    "mutationobserver-shim": "^0.3.2",
    "react-axe": "^3.5.4",
    "react-scripts": "4.0.3",
    "redux-logger": "^3.0.6",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^24.0.2",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^3.8.3",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "homepage": "/app-ui",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
